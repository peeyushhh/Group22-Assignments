#include<bits/stdc++.h>
using namespace std;
int main(){
    string a,b;
    getline(cin,a);
    getline(cin,b);
    int arr1[26]={0},arr2[26]={0};
    bool flag=true;
    for(int i=0;i<a.length();i++)
    {
        arr1[a[i]-'a']++;
    }
    for(int i=0;i<b.length();i++)
    {
        arr2[b[i]-'a']++;
    }
    for(int i=0;i<26;i++)
    {
        if(arr1[i]!=arr2[i])
        {
            flag=false;
            break;
        }
    }
    if(flag)
    cout<<"True";
    else
    cout<<"False";

    return 0;
}
